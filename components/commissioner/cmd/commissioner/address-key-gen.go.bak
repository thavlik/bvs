package main

import (
	"github.com/spf13/cobra"
)

type AddressKeyGenArgs struct {
	Endpoint string
}

var addressKeyGenArgs AddressKeyGenArgs

var addressKeyGenCmd = &cobra.Command{
	Use: "address-key-gen",
	RunE: func(cmd *cobra.Command, args []string) error {
		return nil
	},
}

func init() {
	addressKeyGenCmd.PersistentFlags().StringVar(&addressKeyGenArgs.Endpoint, "endpoint", "http://localhost:80", "service endpoint URI")
	ConfigureCommand(addressKeyGenCmd)
}
